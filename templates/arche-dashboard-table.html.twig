<h2>Statistics - {{key}} </h2>
<table class="display table table-striped" id='db-properties' cellspacing="0" width="100%">
    <thead>
        <tr>
        {% for key,value in cols %}
		<th>{{ key }}</th>
	{% endfor %}
        </tr>
    </thead>
    <tbody>
        {% for key,value in basic %}
            <tr>

	{% if value.class %}
                <td >
                    {{ value.class }}
                </td>
	{% endif %}
	{% if value.property %}
                <td >
                    {{ value.property }}
                </td>
	{% endif %}

	{% if value.cnt_distinct_value %}
                <td >
                    {{ value.cnt_distinct_value }}
                </td>
	{% endif %}
                <td width='20%'>
                    {{ value.cnt }}
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
